{
  "editor.tokenColorCustomizations": 
  {
    "textMateRules": 
    [
      {
        "scope": "log.error",
        "settings": 
        {
          "foreground": "#af1f1f",
          "fontStyle": "bold"
        }
      },
      {
        "scope": "log.warning",
        "settings": 
        {
          "foreground": "#f4ad42",
          "fontStyle": ""
        }
      }
    ]
  },
  "liveServer.settings.CustomBrowser": "firefox",
  "material-icon-theme.folders.color": "#ef5350",
  "search.exclude": 
  {
    "public/**": true,
    "node_modules/**": true
  },
  "spellright.language": [
    "en_GB",
    "pt_PT"
  ],
  "spellright.documentTypes": 
  [
    "css",
    "latex",
    "php",
    "python"
  ]
}
