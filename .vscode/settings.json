{
  "colorize.include": ["*.txt"],
  "colorize.colorized_colors": 
  [
    "HEXA",
    "RGB"
  ],
  "editor.tokenColorCustomizations": 
  {
    "textMateRules": 
    [
      {
        "scope": 
        [
            "comment",
            "log.error"
        ],
        "settings": 
        {
          "fontStyle": "bold"
        }
      },
      {
        "scope": "log.warning",
        "settings": 
        {
          "foreground": "#f4ad42",
          "fontStyle": ""
        }
      }
    ]
  },
  "git.ignoreLimitWarning": true,
  "liveServer.settings.CustomBrowser": "firefox",
  "liveServer.settings.port": 1990,
  "material-icon-theme.folders.color": "#ef5350",
  "search.exclude": 
  {
    "public/**": true,
    "node_modules/**": true
  },
  "spellright.language": 
  [
    "en_GB",
    "pt_PT"
  ],
  "spellright.documentTypes": 
  [
    "css",
    "latex",
    "php",
    "python"
  ],
  "workbench.colorCustomizations": 
  {
    "tab.activeBorder": "#67af7c",
    "titleBar.activeBackground": "#67af7c",
    "titleBar.activeForeground": "#ffffff",
    "statusBar.background": "#67af7c",
    "statusBar.foreground": "#ffffff"
  }
}
